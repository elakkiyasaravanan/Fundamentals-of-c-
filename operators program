}
//New Member in Team Flash
#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    float x1,y1,x2,y2,x3,y3;
    cin>>x1>>y1>>x2>>y2>>x3>>y3;
    cout<<fixed<<setprecision(1)<<((x1+x2+x3)/3)<<"\n"<<fixed<<setprecision(1)<<((y1+y2+y3)/3);
    
}
//The Discount
#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
   float p1,p2,dis,t,f,a,e;
   cin>>p1>>p2>>dis;
   t=p1+p2;
   f=(t*(dis/100));
   a=t-f;
   e=t/dis;
   cout<<fixed<<setprecision(2)<<t<<"\n"<<fixed<<setprecision(2)<<a<<"\n"<<fixed<<setprecision(2)<<f;
    return 0;
}
//#include<iostream>
using namespace std;
int main()
{
    int a,b,c,d,e,f;
    cin>>a>>b>>c;
    d=((b*a)/100);
    cout<<d;
    e=(a-d)*c/100;
    cout<<"\n"<<e;
    f=a-(d+e);
    cout<<"\n"<<f/3;
    
    return 0;
}
//Binfon a Tribal
#include<iostream>
using namespace std;
int main()
{
    int n,year,week,week2,day;
    cin>>n;
    cout<<(n/365);
   year=n%365;
   week=year/7;
   week2=year%7;
   day=week2;
    cout<<"\n"<<week;
    cout<<"\n"<<day;
    return 0;
}
//The Baggings
#include<iostream>
#include<math.h>
using namespace std;
int main()
{
  int a,b,c,d,e;
  cin>>a>>b;
  c=pow((a-3),2);
  d=pow((b-4),2);
  e=sqrt(c+d);
  cout<<e;
  
}
//Chill Fact
#include<iostream>
#include<iomanip>
#include<math.h>
using namespace std;
int main()
{
    int t,v;
    cin>>t>>v;
    cout<<fixed<<setprecision(2)<<(35.74+(0.6215*t)+((0.4275*t)-35.75)*pow(v,0.16));
    
    return 0;
}
