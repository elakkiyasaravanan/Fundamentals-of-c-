//Arithmetic Operator
#include<iostream>
using namespace std;
int main ()
{
    int a,b;
    cin>>a>>b;
cout<<a<<" + "<<b<<" = "<<(a+b)<<"\n"<<a<<" - "<<b<<" = "<<(a-b)<<"\n"<<a<<" * "<<b<<" = "<<(a*b)<<"\n"<<a<<" / "<<b<<" = "<<(a/b)<<"\n"<<a<<" % "<<b<<" = "<<(a%b);
}
//Business Proposal
#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    int p,n,r;
    float d,a;
    cin>>p>>n>>r;//input
    a=((p*n*r)/100);
    cout<<fixed<<setprecision(2)<<a<<"\n"<<fixed<<setprecision(2)<<(a+p);
    d=(0.02*a);
    cout<<"\n"<<fixed<<setprecision(2)<<d;
    cout<<"\n"<<fixed<<setprecision(2)<<(p+a)-d;
}
//Football Stadium
#include<iostream>
using namespace std;
int main ()
{
    int l, b;
    cin>>l>>b;
    cout<<(2*(l+b))<<"\n"<<(l*b);
    
}
//Culturals Training
#include<iostream>
using namespace std;
int main()
{
    int s1,n;
    cin>>s1>>n;
    cout<<(s1/n)<<"\n"<<s1-(n*(s1/n));
}

