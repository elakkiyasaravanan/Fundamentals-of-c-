//Arithmetic Operator
#include<iostream>
using namespace std;
int main ()
{
    int a,b;
    cin>>a>>b;
cout<<a<<" + "<<b<<" = "<<(a+b)<<"\n"<<a<<" - "<<b<<" = "<<(a-b)<<"\n"<<a<<" * "<<b<<" = "<<(a*b)<<"\n"<<a<<" / "<<b<<" = "<<(a/b)<<"\n"<<a<<" % "<<b<<" = "<<(a%b);
}
//Business Proposal
#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    int p,n,r;
    float d,a;
    cin>>p>>n>>r;//input
    a=((p*n*r)/100);
    cout<<fixed<<setprecision(2)<<a<<"\n"<<fixed<<setprecision(2)<<(a+p);
    d=(0.02*a);
    cout<<"\n"<<fixed<<setprecision(2)<<d;
    cout<<"\n"<<fixed<<setprecision(2)<<(p+a)-d;
}
