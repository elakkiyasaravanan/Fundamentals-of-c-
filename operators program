//Swapping two numbers
#include<iostream>
using namespace std;
int main()
{
    int a,b,c;
    cin>>a>>b;
    c=a;
    a=b;
    b=c;
    cout<<a<<"\n"<<b;
    
}
//Swapping two numbers without third variable
#include<iostream>
using namespace std;
int main()
{
    int a,b;
    cin>>a>>b;
    a=a+b;
    b=a-b;
    a=a-b;
    cout<<a<<"\n"<<b;
}
//Average Calculation
#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    float a,b,c,d,e;
    cin>>a>>b>>c>>d>>e;
    cout<<fixed<<setprecision(2)<<(a+b+c+d+e)/5;
}
//Area Calculation
#include <iostream>
#include<iomanip>
using namespace std;
int main()
{
    int side,length,breadth,a,b;
    float c,radius;
    cin>>side>>length>>breadth>>radius;
    c=(3.14*radius*radius);
    b=length*breadth;
    a=side*side;
    cout<<a<<"\n"<<b<<"\n"<<fixed<<setprecision(2)<<c;
}
