//spliting into teams
#include<iostream>
using namespace std;
int main()
{
    int a,b,c;
    cin>>a>>b;
    c=a-((a/b)*b);
    cout<<"The number of students in each team is "<<(a/b)<<" and left out is "<<c;
}
//Ternary Operator
#include<iostream>
using namespace std;
int main()
{
    int a;
    cin>>a;
    cout<<((a%2==0)?"Even":"Odd");
}
//Fencing The Ground
#include<iostream>
using namespace std;
int main()
{
    int a,b,l,p;
    cin>>a>>b;
    l=((2*a)+(2*b));
    p=a*b;
    cout<<"Required length is "<<l<<"m"<<"\nRequired quantity of carpet is "<<p<<"sqm";
}
//Jack Sparrowâ€™s Treasure Hunt
#include<iostream>
using namespace std;
int main()
{
 int a,p,o,u;
 cin>>a;
 p=a/1000;
 o=a%10;
 u=p+o;
 cout<<u;
}
//Profit Calculation
#include<iostream>
using namespace std;
int main()
{
    int x,a,b,r;
    cin>>x>>a>>b;
    r=(x*a)-(x*b+100);
    cout<<"The profit obtained is Rs."<<r;
}
